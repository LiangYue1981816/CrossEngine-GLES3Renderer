<Material>
	<Cull enable="true" front_face="GL_CCW" />
	<Depth enable_test="true" enable_write="true" depth_func="GL_LESS" />
	<Blend enable="false" src_factor="GL_SRC_ALPHA" dst_factor="GL_ONE_MINUS_SRC_ALPHA" />
	<Program vertex_file_name="BRDF_PbrIBL.vert.bin" fragment_file_name="BRDF_PbrIBL.frag.bin" />
	<Texture2D file_name="Robot.dds" name="texAlbedo" min_filter="GL_LINEAR_MIPMAP_NEAREST" mag_filter="GL_LINEAR" address_mode="GL_CLAMP_TO_EDGE" />
	<Texture2D file_name="Robot_N.dds" name="texNormal" min_filter="GL_LINEAR_MIPMAP_NEAREST" mag_filter="GL_LINEAR" address_mode="GL_CLAMP_TO_EDGE" />
	<Texture2D file_name="Robot_RM.dds" name="texRoughMetallic" min_filter="GL_LINEAR_MIPMAP_NEAREST" mag_filter="GL_LINEAR" address_mode="GL_CLAMP_TO_EDGE" />
	<TextureCubeMap file_name="Cube.dds" name="texEnv" min_filter="GL_LINEAR_MIPMAP_LINEAR" mag_filter="GL_LINEAR" address_mode="GL_CLAMP_TO_EDGE" />
</Material>